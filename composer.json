{
    "name": "bit3/composer-global-depends",
    "description": "Analyse which package depends on another one globally.",
    "license": "MIT",
    "authors": [
        {
            "name":"Tristan Lins",
            "email":"tristan@lins.io",
            "homepage":"https://tristan.lins.io",
            "role":"Developer"
        }
    ],
    "support":{
        "issues":"https://github.com/bit3/composer-global-depends/issues",
        "source":"https://github.com/bit3/composer-global-depends"
    },
    "autoload": {
        "psr-4": {
            "Bit3\\ComposerGlobalDepends\\" : "src"
        }
    },
    "require": {
        "php": ">=5.4",
        "symfony/console": "~2.3",
        "composer/composer": "dev-master"
    },
    "require-dev": {
        "phpcq/phpcq": "~1.0",
        "phpcq/author-validation": "~1.0",
        "phpcq/autoload-validation": "~1.0",
        "phpcq/branch-alias-validation": "~1.0",
        "phpcq/coding-standard": "~1.0",
        "phpcq/travis-configuration-check": "~1.0",
        "bit3/git-php": "~1.0",
        "squizlabs/php_codesniffer": "~1.0",
        "pdepend/pdepend": "2.0.*",
        "sebastian/phpcpd": "~1.4",
        "phploc/phploc": "~2.0",
        "phpmd/phpmd": "~2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.x-dev"
        }
    },
    "bin": ["bin/composer-global-depends.php"],
    "prefer-stable": true,
    "minimum-stability": "dev"
}